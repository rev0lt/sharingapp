package network;

public class UserData {
    String username, password, email, facebook, twitter, linkedin;
    int phone;
    

    public UserData() {
        username = "";
        password = "";
        email = "";
        facebook = "";
        twitter = "";
        linkedin = "";
        phone = 0;
    }
    
    public Object[] serialize() {
        Object[] data = new Object[7];
        data[0] = username;
        data[1] = password;
        data[2] = email;
        data[3] = facebook;
        data[4] = twitter;
        data[5] = linkedin;
        data[6] = (Integer) phone;
        return data;
    }
    
    public Object[] deserialize(Object[]) {
        Object[] data = new Object[7];
        data[0] = username;
        data[1] = password;
        data[2] = email;
        data[3] = facebook;
        data[4] = twitter;
        data[5] = linkedin;
        data[6] = (Integer) phone;
        return data;
    }
    
    public String getUsername() {
        return username;
    }
    
    public void setUsername(String s) {
        username = s;
    }
    
    public String getPassword() {
        return password;
    }
    
    public void setPassword(String s) {
        password = s;
    }
}
